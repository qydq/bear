<resources>
    <!--()Mlmmlm-->
    <!--夜间主题-->
    <style name="DayNightTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimary">@color/ColorTransparent</item>
        <item name="colorPrimaryDark">@color/anBarBackground</item>
        <item name="colorAccent">@color/anLaunchBackground</item>
        <!--        <item name="android:windowTranslucentStatus">true</item>-->
        <item name="android:windowFullscreen">false</item>

        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/anBackground</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>

        <item name="anBarBackground">@color/anBarBackground</item>
        <item name="anBackground">@drawable/in_background_angle</item>
        <item name="anNavBackground">@color/anNavBackground</item>
        <item name="anLineBackground">@color/anLineBackground</item>

        <item name="anOptBackground">@drawable/in_selector</item>
        <item name="anOptMaterialBackground">@color/anLineBackground</item>
        <item name="anOppBackground">@color/anLineBackground</item>
        <item name="anOppMaterialBackground">@color/anLineBackground</item>

        <item name="anBarTextColor">@color/anBarTextColor</item>
        <item name="anTextColor">@color/anTextColor</item>
        <item name="anDeepTextColor">@color/anDeepTextColor</item>
        <item name="anLightTextColor">@color/anLightTextColor</item>
        <item name="anNavTextColor">@color/anNavTextColor</item>
    </style>
</resources>
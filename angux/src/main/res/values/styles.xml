    <!--自定义dialog背景全透明无边框theme -->
    <style name="an_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item><!--是否去除边框-->
        <item name="android:background">@android:color/transparent</item><!--背景颜色设置为透明-->
        <item name="android:windowBackground">@android:color/transparent</item><!--背景颜色及和透明程度-->
        <item name="android:windowNoTitle">true</item><!--是否去除标题 -->
        <item name="android:backgroundDimEnabled">true</item><!--是否模糊-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
    </style>

    <style name="TransparentTheme" parent="an_dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsFloating">false</item>
    </style>

    <!--全屏有阴影-->
    <!--<item name="android:alpha">0.96</item>-->
    <style name="an_dialog.loading_dim" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowBackground">@android:color/transparent</item><!--背景颜色及和透明程度-->
    </style>
    <!--全屏无阴影-->
    <!--<item name="android:alpha">0.96</item>-->
    <style name="an_dialog.loading_nodim" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowBackground">@android:color/transparent</item><!--背景颜色及和透明程度-->
    </style>

    <!--        <item name="android:windowBackground">@drawable/base_bg_loading</item>
-->
    <style name="an_dialog.loadding_standard">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:alpha">0.96</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!--        <item name="android:alpha">0.9</item>
-->
    <style name="an_dialog.loadding_animation">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="an_dialog.lodding_progress">
        <item name="android:backgroundDimEnabled">true</item>
        <item name="colorAccent">#ffE91E63</item>
    </style>
    <!--原custom_dialog-->
    <style name="an_dialog.middle_pure_image">
        <item name="android:alpha">0.96</item>
    </style>

    <style name="an_dialog.middle_list">
        <item name="android:backgroundDimAmount">0.2</item>
    </style>

    <style name="an_dialog.middle_download_center" />

    <!--原time_dialog-->
    <style name="an_dialog.time_selector">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/an_color_white</item>

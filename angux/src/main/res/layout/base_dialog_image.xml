<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/an_match_wrap"
    android:background="@color/ColorTransparent">

    <RelativeLayout
        android:id="@+id/frameLayout"
        android:layout_width="300dp"
        android:layout_height="350dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/an_dimen_margintop_small"
        android:layout_marginBottom="@dimen/an_dimen_margintop_small">

        <ImageView
            android:id="@+id/ivPrimary"
            android:layout_width="300dp"
            android:layout_height="300dp" />

        <ImageView
            android:id="@+id/ivCancle"
            style="@style/an_wrap_wrap"
            android:layout_below="@+id/ivPrimary"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:padding="12dp"
            android:src="@drawable/in_selector_clear" />
    </RelativeLayout>


</RelativeLayout>